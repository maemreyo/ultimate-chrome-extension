{
  "name": "ultimate-ai-chrome-extension",
  "displayName": "Ultimate AI Chrome Extension",
  "version": "2.0.0",
  "description": "The most comprehensive AI-powered Chrome Extension with advanced storage, content extraction, and analysis capabilities",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "ai",
    "content-extraction",
    "analysis",
    "plasmo",
    "react",
    "typescript",
    "supabase",
    "stripe"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:chrome": "plasmo build --target=chrome-mv3",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "build:edge": "plasmo build --target=edge-mv3",
    "package": "plasmo package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "plasmo build --analyze",
    "clean": "rimraf .plasmo build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@matthew.ngo/ai-toolkit": "^1.0.0",
    "@matthew.ngo/chrome-storage": "^1.0.0",
    "@matthew.ngo/analysis-toolkit": "^1.0.0",
    "@matthew.ngo/content-extractor": "^1.0.0",
    "@huggingface/inference": "^3.15.0",
    "@plasmohq/messaging": "^0.6.1",
    "@plasmohq/storage": "^1.9.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@stripe/stripe-js": "^2.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.12.2",
    "@tensorflow/tfjs": "^4.22.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compromise": "^14.14.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dexie": "^4.0.11",
    "franc": "^6.2.0",
    "framer-motion": "^10.16.5",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.10.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.294.0",
    "marked": "^15.0.4",
    "natural": "^8.0.1",
    "next": "^14.0.3",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.4.5",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "sanitize-html": "^2.14.0",
    "sentiment": "^5.0.2",
    "sharp": "^0.34.2",
    "stripe": "^14.5.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "turndown": "^7.2.0",
    "uuid": "^11.1.0",
    "verror": "^1.10.1",
    "zod": "^3.25.42",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@plasmohq/rps": "^1.8.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/chrome": "^0.0.253",
    "@types/crypto-js": "^4.2.2",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.202",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.0",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/sanitize-html": "^2.13.0",
    "@types/sentiment": "^5.0.4",
    "@types/turndown": "^5.0.5",
    "@types/verror": "^1.10.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "plasmo": "^0.90.5",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "storage",
      "tabs",
      "contextMenus",
      "notifications",
      "alarms",
      "identity",
      "sidePanel",
      "activeTab",
      "scripting"
    ],
    "optional_permissions": [
      "bookmarks",
      "history",
      "downloads",
      "webNavigation",
      "clipboardRead",
      "clipboardWrite"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*",
          "tabs/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
